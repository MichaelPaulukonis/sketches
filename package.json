{
  "name": "sketches",
  "version": "1.0.13",
  "description": "p5js (etc) web sketches",
  "author": "Michael Paulukonis",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelPaulukonis/sketches.git"
  },
  "keywords": [
    "p5",
    "text",
    "paint",
    "nuxt"
  ],
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "clean": "rm -rf dist",
    "deploy": "gh-pages -t -d dist",
    "bump": "npm version patch",
    "publish": "npm run bump && DEPLOY_ENV=GH_PAGES npm run generate && npm run deploy",
    "lint:fix": "standard --fix --ext .js,.vue",
    "lint": "standard --ext .js,.vue"
  },
  "dependencies": {
    "nuxt": "^2.0.0",
    "p5": "^0.9.0",
    "vue-draggable-resizable": "^2.0.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.1.2",
    "@nuxtjs/eslint-module": "^1.1.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.3",
    "cheerio": "^1.0.0-rc.3",
    "eslint": "^6.6.0",
    "eslint-config-p5js": "^1.0.1",
    "eslint-plugin-nuxt": "^0.4.3",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "opn": "^6.0.0",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "standard": "^14.3.1"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}