{
  "name": "sketches",
  "version": "1.0.15",
  "description": "p5js (etc) web sketches",
  "author": "Michael Paulukonis",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelPaulukonis/sketches.git"
  },
  "keywords": [
    "p5",
    "text",
    "paint",
    "nuxt"
  ],
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "clean": "rm -rf dist",
    "deploy": "gh-pages -t -d dist",
    "bump": "npm version patch",
    "publish": "npm run bump && DEPLOY_ENV=GH_PAGES npm run generate && npm run deploy",
    "lint:fix": "eslint --fix --ext .js,.vue",
    "lint": "eslint --ext .js,.vue"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "FitText-UMD": "^1.0.0",
    "nuxt": "^2.17.3",
    "p5": "^1.9.0",
    "vue-draggable-resizable": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.27.0",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.33.0",
    "gh-pages": "^6.3.0",
    "opn": "^6.0.0",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "sass": "^1.71.0",
    "sass-loader": "^10.1.1"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  }
}
